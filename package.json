{
  "name": "@yenbekbay/surmount",
  "version": "0.0.0",
  "private": true,
  "repository": "git@github.com:yenbekbay/surmount.git",
  "license": "GPL-3.0",
  "scripts": {
    "typecheck": "tsc --noEmit --pretty",
    "dev": "parcel public/index.html",
    "build": "parcel build public/index.html"
  },
  "cache": {
    "inDev": true,
    "swDest": "public/service-worker.js"
  },
  "prettier": {
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn typecheck"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "matter-js": "0.14.2",
    "poly-decomp": "0.3.0",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.2.0",
    "@types/matter-js": "0.10.2",
    "@types/node": "^12.0.8",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "autoprefixer": "9.6.0",
    "husky": "2.4.1",
    "lint-staged": "8.2.1",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-inlinesvg": "0.1.1",
    "parcel-plugin-sw-cache": "0.3.1",
    "prettier": "1.18.2",
    "tailwindcss": "1.0.4",
    "typescript": "3.5.2"
  }
}
